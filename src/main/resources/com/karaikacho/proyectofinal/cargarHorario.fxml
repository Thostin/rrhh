<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karaikacho.proyectofinal.CargarHorarioController">
   <children>
      <Pane layoutX="30.0" layoutY="26.0" prefHeight="340.0" prefWidth="543.0" style="-fx-background-color: gray;">
         <children>
            <TextField fx:id="txtHoraInicio" layoutX="14.0" layoutY="39.0" promptText="HH:mm" />
            <TextField fx:id="txtHoraFin" layoutX="304.0" layoutY="39.0" promptText="HH:mm" />
            <TextField fx:id="txtFuncionario" disable="true" layoutX="14.0" layoutY="144.0" prefHeight="26.0" prefWidth="202.0" />
            <Button fx:id="btnBuscarFuncionario" layoutX="224.0" layoutY="144.0" mnemonicParsing="false" onAction="#buscarFuncionario" text="..." />
            <TextField fx:id="txtSala" disable="true" layoutX="304.0" layoutY="144.0" />
            <Button fx:id="btnBuscarSala" layoutX="473.0" layoutY="144.0" mnemonicParsing="false" onAction="#buscarSala" text="..." />
            <ComboBox fx:id="cmbEsCompensatorio" layoutX="21.0" layoutY="238.0" prefWidth="150.0" />
            <Label layoutX="20.0" layoutY="14.0" text="Hora Inicio:" />
            <Label layoutX="306.0" layoutY="113.0" text="Sala:" />
            <Label layoutX="20.0" layoutY="113.0" text="Funcionario:" />
            <Label layoutX="304.0" layoutY="15.0" text="Hora Fin:" />
            <Label layoutX="20.0" layoutY="216.0" text="Es compensatorio:" />
            <Button fx:id="btnConfirmar" layoutX="284.0" layoutY="277.0" mnemonicParsing="false" onAction="#confirmar" text="Confirmar" />
            <Button fx:id="btnCancelar" layoutX="413.0" layoutY="277.0" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
            <ComboBox fx:id="cmbDia" layoutX="304.0" layoutY="238.0" prefWidth="150.0" />
            <Label layoutX="308.0" layoutY="216.0" text="Dia:" />
         </children>
      </Pane>
   </children>
</AnchorPane>
